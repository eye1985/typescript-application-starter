{
  "name": "ts-starter-kit",
  "version": "1.0.0",
  "description": "A starter pack for TS applications",
  "main": "app.js",
  "author": "Erik",
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack -d",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
